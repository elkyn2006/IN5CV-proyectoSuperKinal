<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="950.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.elkynsamayoa.controller.TicketSoporteController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="950.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/Menu%20ticket%20soporte.png" />
         </image>
      </ImageView>
      <ComboBox fx:id="cmbEstatus" layoutX="27.0" layoutY="463.0" prefWidth="150.0" />
      <TextField fx:id="textId" editable="false" layoutX="33.0" layoutY="140.0" promptText="Ticket ID" />
      <TextArea fx:id="textDescripcion" layoutX="27.0" layoutY="245.0" prefHeight="177.0" prefWidth="179.0" wrapText="true" />
      <ComboBox fx:id="cmbClientes" layoutX="280.0" layoutY="140.0" prefWidth="150.0" />
      <ComboBox fx:id="cmbFactura" layoutX="519.0" layoutY="140.0" prefWidth="150.0" />
      <TableView fx:id="tblTicketSoporte" layoutX="280.0" layoutY="203.0" prefHeight="323.0" prefWidth="542.0">
        <columns>
          <TableColumn fx:id="colTicketId" prefWidth="75.0" text="TicketId" />
          <TableColumn fx:id="colDescripcion" prefWidth="150.0" text="Descripcion" />
            <TableColumn fx:id="colEstatus" prefWidth="110.0" text="Estatus" />
            <TableColumn fx:id="colCliente" prefWidth="130.0" text="Cliente" />
            <TableColumn fx:id="colFactura" prefWidth="75.0" text="Factura" />
        </columns>
      </TableView>
      <Button fx:id="btnBack" layoutX="33.0" layoutY="511.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="52.0" prefWidth="79.0" style="-fx-background-radius: 15;" text="Regresar" />
      <Button fx:id="btnGuardar" layoutX="127.0" layoutY="511.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="52.0" prefWidth="79.0" style="-fx-background-radius: 15;" text="Guardar" />
   </children>
</AnchorPane>
